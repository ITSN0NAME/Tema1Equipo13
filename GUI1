package Gui;

import javax.swing.JFrame;

public class Gui1 extends JFrame{
    //constantes y componentes
    public Gui1(){
        super("Ventana usando Herencia");//Establece el titulo de la ventana
        setSize(370,450);//Establece el tama침o de mi ventana
        setLocationRelativeTo(null);/*La ventana se posiciona en el centro de la pantalla*/
        setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);//Establece que el boton de cerrar permitir치 salir de la aplicaci칩n
        setResizable(false);//Establece que el tama침o de la ventana no se pueda cambiar
        setVisible(true);
    }
}
